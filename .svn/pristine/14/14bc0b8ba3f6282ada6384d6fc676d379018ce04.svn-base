<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="edu.wm.cs.cs301.amazebyeyosyas">

    <application
        android:allowBackup="true"
        android:icon="@drawable/a_maze_ing_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <!--android:theme="@style/AppTheme" -->

        <activity android:name=".ui.AMazeActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.GeneratingActivity"
            android:theme="@style/AppTheme"/>
        <activity android:name=".ui.PlayActivity"
            android:theme="@style/AppTheme"/>
        <activity android:name=".ui.FinishActivity"
            android:theme="@style/AppTheme"/>
        <activity android:name=".ui.SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>